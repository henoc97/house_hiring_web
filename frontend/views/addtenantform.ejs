<div class="new_customers">
    <div class="card_header">
      <h2>Enregistrer un locataire</h2>
    </div>
    <form id="property-form">
        <div class="form-group">
            <label for="property">Propriétés à louer:</label>
            <select id="property" name="property" required>
              <option value="house1">House1</option>
              <option value="House2">House2</option>
              <option value="House3">House3</option>
            </select>
          </div>
        <div class="form-group">
            <label for="name">Nom:</label>
            <input type="text" id="name" name="name" required>
        </div>
        <div class="form-group">
            <label for="lastname">Prénoms:</label>
            <input type="text" id="lastname" name="lastname" required>
        </div>
        <div class="form-group">
            <label for="contactmoov">Contact moov:</label>
            <input type="text" id="contactmoov" name="contactmoov" >
        </div>
        <div class="form-group">
            <label for="contacttg">Contact Togocel:</label>
            <input type="text" id="contacttg" name="contacttg" >
        </div>
        
        <button type="submit">Enregistrer</button>
    </form>
  </div>
  