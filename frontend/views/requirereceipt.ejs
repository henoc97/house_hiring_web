<div class="new_customers">
    <div class="card_header">
      <h2>Commander un reçu </h2>
    </div>
    <form id="property-form">
        <div class="form-group">
            <label for="property">Locataire & Logement:</label>
            <select id="property-option" name="property" required>
              <option value="house1">House1</option>
              <option value="House2">House2</option>
              <option value="House3">House3</option>
            </select>
        </div>
        <div class="form-group">
            <label for="description">Coût:</label>
            <input type="text" id="description" name="description" required>
        </div>
        <div class="form-group">
            <label for="cost">Mois:</label>
            <input type="number" id="cost" name="cost" required>
        </div>
        
        <button type="submit">Enregistrer</button>
    </form>
  </div>
  
  